			}

			//Insert Promo Drawer Library before document-ready
			var promoDrawerScript = document.createElement("script");
			promoDrawerScript.type = 'text/javascript';
			promoDrawerScript.onload = function() {
				IS_PROMO_DRAWER_READY = true;
				jQuery(document).trigger("wcd:render-promo-drawer");
			}
			promoDrawerScript.src = '/Asset_Archive/GPWeb/content/promo_drawer/promo_drawer_v2-6-7_min.js';
			promoDrawerTargetContainer.appendChild(promoDrawerScript);
		}
	};


	//this event fires when PAS and Promo Drawer events have finished.
	//we need to wait for both events to finish before we can show the right promo drawer
	jQuery(document).on("wcd:render-promo-drawer",function(){

		//only continue if both events are done
		if(IS_PROMO_DRAWER_READY && !IS_DRAWER_INITIALIZED){

			//default json url
			var PROMO_DRAWER_URL = "/Asset_Archive/ONWeb/content/0014/617/146/config.js?2";

			if ('insertPromoDrawer' in window) {
				if( IS_MOBILE ) {
					insertPromoDrawer(PROMO_DRAWER_URL,PROMO_DRAWER_HIDDEN_CID,600,'partial');
					IS_DRAWER_INITIALIZED = true;
				}else {
					insertPromoDrawer(PROMO_DRAWER_URL,PROMO_DRAWER_HIDDEN_CID,600);
					IS_DRAWER_INITIALIZED = true;
				}
			}
		}
	});

	// Dynamically load the promo drawer JS library
	var locPath = location.pathname;
	if( locPath.match(/(home|division|category|subDivision).do/) || locPath=="/" || location.search.indexOf("cid=" + PROMO_DRAWER_HIDDEN_CID) >= 0 ) {
		if(locPath !== "/browse/category.do"){
			renderPromoDrawerAndInsertLibrary();
		}else{
			jQuery(document).on("ready",renderPromoDrawerAndInsertLibrary);
		}
	}

</script>
