					+ "		<div id='promoDrawer__footer'></div>"
					+ " </div>"
					;

				promoDrawerTargetContainer.insertAdjacentHTML("afterbegin",promoDrawerLayoutHTML);

				//Close Drawer and display it
				var jPromoDrawerWrapper = jQuery("#promoDrawer__wrapper");
				var translateStr = "translate3d(0," + (jPromoDrawerWrapper.outerHeight() - jQuery("#promoDrawer__handlebar").outerHeight()) + "px,0)";
				jPromoDrawerWrapper.css({
