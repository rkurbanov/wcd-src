	var PROMO_DRAWER_HEADER_TEXT = "EXTRA 20% OFF EVERYTHING, NO EXCLUSIONS!";

	var renderPromoDrawerAndInsertLibrary = function(){
		//insert promo drawer js before document-ready and in some child of body tag
		// var promoDrawerTargetContainer = document.querySelector("#responsiveUniversalBar") || document.querySelector(".universal-nav");
		var promoDrawerTargetContainer = document.querySelector("#responsiveUniversalBar") || document.querySelector("body");

		if(promoDrawerTargetContainer){

			if(location.search.indexOf("cid=" + PROMO_DRAWER_HIDDEN_CID) < 0){
				//Insert promo Drawer Layout - CHANGE BRAND ABBREV below
				var device = !IS_MOBILE ? "desktop" : "mobile";
				var promoDrawerLayoutHTML = ""
					+ "	<div id='promoDrawer__wrapper' data-state='closed' class='" + device + " ON EN'>"
					+ "		<div id='promoDrawer__handlebar'>"
					+ "			<div id='promoDrawer__handlebar__text'>"
